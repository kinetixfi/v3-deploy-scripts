{
  "name": "@kinetix/deploy-v3-scripts",
  "version": "1.0.0",
  "description": "Deploy Kinetix V3 smart contracts",
  "bin": "dist/index.js",
  "publishConfig": {
    "access": "restricted"
  },
  "main": "dist/index.js",
  "scripts": {
    "test": "mocha",
    "build": "NODE_OPTIONS=--openssl-legacy-provider ncc build index.ts -o dist -m",
    "postbuild": "cat shebang.txt dist/index.js > dist/index.cmd.js && mv dist/index.cmd.js dist/index.js",
    "prestart": "NODE_OPTIONS=--openssl-legacy-provider yarn build",
    "start": "NODE_OPTIONS=--openssl-legacy-provider node dist/index.js",
    "prepublishOnly": "yarn build",
    "publish": "npm version && npm publish --no-git-checks"
  },
  "files": [
    "dist/"
  ],
  "engines": {
    "node": ">=12.18.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kinetixfi/v3-deploy-scripts"
  },
  "author": {
    "email": "contact@kinetix.finance",
    "name": "Kinetix Labs",
    "url": "https://kinetix.finance"
  },
  "license": "GPL-3.0-or-later",
  "homepage": "https://kinetix.finance",
  "devDependencies": {
    "@ethersproject/abstract-signer": "^5.5.0",
    "@ethersproject/address": "^5.5.0",
    "@ethersproject/bignumber": "^5.5.0",
    "@ethersproject/constants": "^5.5.0",
    "@ethersproject/contracts": "^5.5.0",
    "@ethersproject/providers": "^5.5.1",
    "@ethersproject/wallet": "^5.5.0",
    "@kinetix/v3-core-smart-contracts": "^1.0.0",
    "@kinetix/v3-periphery": "^1.0.0",
    "@kinetix/v3-staker": "^1.0.0",
    "@openzeppelin/contracts": "3.4.1-solc-0.7-2",
    "@types/chai": "^4.2.12",
    "@types/mocha": "^8.0.3",
    "@types/node": "^14.6.3",
    "@uniswap/sdk-core": "^1.0.8",
    "@uniswap/swap-router-contracts": "1.1.0",
    "@vercel/ncc": "^0.33.1",
    "chai": "^4.2.0",
    "commander": "^6.1.0",
    "ganache-cli": "^6.10.1",
    "immer": "^7.0.8",
    "mocha": "^8.1.3",
    "prettier": "^2.1.1",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.2",
    "v3-periphery-1_3_0": "npm:@uniswap/v3-periphery@1.3.0"
  },
  "dependencies": {
    "dotenv": "^16.3.1"
  }
}
